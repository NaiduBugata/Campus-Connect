{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "frontend/dist"
      }
    }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/webpushr-sw.js", "dest": "/webpushr-sw.js" },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
